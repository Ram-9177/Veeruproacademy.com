{
  "name": "veeru-pro-academy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "sh -c 'DIRECT_URL=${DIRECT_URL:-$DATABASE_URL} prisma generate --schema=./prisma/schema.prisma' && next build",
    "vercel-build": "node scripts/vercel-build.mjs && next build",
    "type-check": "tsc --noEmit",
    "start": "next start",
    "export": "next export",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test": "jest __tests__ --config=./jest.config.cjs --runInBand",
    "prepare-test-assets": "node scripts/copy-axe.js",
    "e2e": "npm run prepare-test-assets && playwright test",
    "e2e:smoke": "npm run prepare-test-assets && npx playwright test tests/smoke.spec.ts",
    "health-check": "node scripts/health-check.js",
    "start:health": "bash scripts/start-and-healthcheck.sh",
    "postinstall": "sh -c 'DIRECT_URL=${DIRECT_URL:-$DATABASE_URL} prisma generate --schema=./prisma/schema.prisma' || echo 'prisma generate failed; continuing'",
    "dev:all": "next dev",
    "all": "bash ./run.sh",
    "db:migrate": "prisma migrate dev --schema=./prisma/schema.prisma",
    "db:generate": "prisma generate --schema=./prisma/schema.prisma",
    "db:seed": "tsx scripts/seed-admin.ts",
    "db:studio": "prisma studio --schema=./prisma/schema.prisma"
  },
